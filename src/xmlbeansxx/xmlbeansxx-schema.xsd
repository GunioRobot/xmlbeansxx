<!--
    This schema is used to provide serialization and parsing 
    of xml schema types without global element defined. 
-->
<xs:schema xmlns="http://xmlbeans.apache.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xmlbeans.apache.org" elementFormDefault="qualified">
    <xs:complexType name="xml-array">
        <xs:sequence>
            <xs:element name="e" type="xs:anyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>

